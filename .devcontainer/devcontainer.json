{
    "name": "Swift",
    "image": "swift:5.7",
    "runArgs": [
      "--cap-add=SYS_PTRACE",
      "--security-opt",
      "seccomp=unconfined"
    ],
    "settings": {
      "lldb.adapterType": "bundled",
      "lldb.executable": "/usr/bin/lldb",
      "sde.languageservermode": "sourcekite",
      "swift.path.sourcekite": "/usr/local/bin/sourcekite"
    },
    "extensions": [
      "sswg.swift-lang"
    ]
  }
  